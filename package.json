{
  "name": "create-isomorphic-destructurable",
  "version": "0.2.0",
  "packageManager": "pnpm@8.5.1",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "contributors": [
    "Ray <nn_201312@163.com> (@so1ve)"
  ],
  "description": "Create isomorphic destructurables. Credits to [@antfu](https://antfu.me/posts/destructuring-with-object-or-array).",
  "keywords": [
    "isomorphic",
    "object",
    "array",
    "destruct"
  ],
  "homepage": "https://github.com/so1ve/create-isomorphic-destructurable#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/create-isomorphic-destructurable.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/create-isomorphic-destructurable/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && pkgroll",
    "watch": "pkgroll --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@so1ve/eslint-config": "^0.90.0",
    "@types/node": "^18.16.13",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "pkgroll": "^1.10.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4",
    "vite": "^4.3.8",
    "vitest": "^0.31.1"
  }
}
