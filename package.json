{
  "name": "cride",
  "version": "0.5.0-beta.0",
  "packageManager": "pnpm@8.10.0",
  "author": "Ray <i@mk1.io> (@so1ve)",
  "description": "Create isomorphic destructurables.",
  "keywords": [
    "array",
    "destruct",
    "isomorphic",
    "object"
  ],
  "homepage": "https://github.com/so1ve/cride#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/so1ve/cride.git"
  },
  "bugs": {
    "url": "https://github.com/so1ve/cride/issues"
  },
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && pkgroll",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "pkgroll --watch"
  },
  "dependencies": {
    "klona": "^2.0.6"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.3",
    "@so1ve/eslint-config": "^0.110.0",
    "@types/node": "^18.16.13",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "pkgroll": "^1.10.0",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.1-rc",
    "vite": "^4.3.8",
    "vitest": "^0.32.0"
  }
}
